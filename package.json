	"bugs": {
		"url": "https://github.com/PhilipSkinner/diskstats/issues"
	},
	"homepage": "https://github.com/PhilipSkinner/diskstats",
	"devDependencies": {
		"jest": "^23.6.0",
		"sinon": "^6.3.5"
	}
